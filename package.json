{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat.",
  "main": "backend/index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js && ",
    "dev": "nodemon backend/index.js --exec babel-node",
    "test": "set NODE_ENV=test&& npm run build && npm run sequeliz && nyc --reporter=html --reporter=text mocha --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "npm run clean && babel backend -d ./dist",
    "clean": "rm -rf dist && mkdir dist",
    "serve": "node dist/index.js",
    "sequeliz": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "engines": {
    "node": "10.15.2",
    "npm": "6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komsic/BookAMeal.git"
  },
  "keywords": [
    "meal",
    "food"
  ],
  "author": "komsic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/komsic/BookAMeal/issues"
  },
  "homepage": "https://github.com/komsic/BookAMeal#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
